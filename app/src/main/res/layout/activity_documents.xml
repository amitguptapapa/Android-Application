<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.LandmarkMapActivity"
    android:orientation="vertical">

    <include
        android:id="@+id/h1"
        layout="@layout/form_header"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/_50sdp" />

    <LinearLayout
        android:id="@+id/llDocsAttached"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_5sdp">

        <ExpandableListView
            android:id="@+id/simpleExpandableListDocuments"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:divider="@color/greyLight"
            android:dividerHeight="@dimen/_1sdp" />

    </LinearLayout>
</LinearLayout>